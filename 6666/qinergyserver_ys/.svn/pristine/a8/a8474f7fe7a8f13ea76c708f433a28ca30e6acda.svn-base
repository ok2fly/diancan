package com.qinergy.dto.integratmonitor;

import java.io.Serializable;
import java.util.Date;

/**
 * 
 * @desc: 微网系统 bean
 * @author: Qist
 * @date: 2017年10月27日
 */
public class MicroNetworkSysDto implements Serializable{
	private static final long serialVersionUID = 6553455013697935182L;
	
	
	private String id;
	private String equ_num; // 设备编号
	private Integer stat; /** 运行状态 0：通讯中断、1：正常运行、2：正常停止、3：告警运行、4：故障停止*/
	private Integer mode; /** 工作模式 1：光伏平滑/2：削峰填谷/3：负荷追踪/4：PCC点平滑*/
	private Integer gridStat; /** 并离网状态 0：离网，1：并网*/
	private Integer faultNum; // 告警码
	private Integer signal; // 信号强度
	private Double uplink; // 上行流量
	private Double downlink; // 下行流量
	private String iccid;
	private String phone;
	private Double pGrid; // 电网总有功
	private Double qGrid; // 电网总无功
	private Double pLoad; // 负荷总有功
	private Double qLoad; // 负荷总无功
	private Double pBatts; // 储能总有功
	private Double qBatts; // 储能总无功
	private Double pPV; // 光伏总有功
	private Double qPV; // 光伏总无功
	private Double pEV; // 充电桩总有功
	private Double qEV; // 充电桩总无功
	private Double power4Grid; // 电网输入电量
	private Double power2Grid; // 馈网电量
	private Double powerLoad; // 负荷用电电量
	private Double powerCharge; // 储能充电电量
	private Double powerDisCharge; // 储能放电电量
	private Double powerPV; // 光伏发电电量
	private Double powerEV; // 充电桩用电电量

	private Integer healthStat; // 健康状态
	private Date crtTim; // 添加时间
	
	
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getEqu_num() {
		return equ_num;
	}
	public void setEqu_num(String equ_num) {
		this.equ_num = equ_num;
	}
	public Integer getStat() {
		return stat;
	}
	public void setStat(Integer stat) {
		this.stat = stat;
	}
	public Integer getMode() {
		return mode;
	}
	public void setMode(Integer mode) {
		this.mode = mode;
	}
	public Integer getGridStat() {
		return gridStat;
	}
	public void setGridStat(Integer gridStat) {
		this.gridStat = gridStat;
	}
	public Integer getFaultNum() {
		return faultNum;
	}
	public void setFaultNum(Integer faultNum) {
		this.faultNum = faultNum;
	}
	public Integer getSignal() {
		return signal;
	}
	public void setSignal(Integer signal) {
		this.signal = signal;
	}
	public Double getUplink() {
		return uplink;
	}
	public void setUplink(Double uplink) {
		this.uplink = uplink;
	}
	public Double getDownlink() {
		return downlink;
	}
	public void setDownlink(Double downlink) {
		this.downlink = downlink;
	}
	public String getIccid() {
		return iccid;
	}
	public void setIccid(String iccid) {
		this.iccid = iccid;
	}
	public String getPhone() {
		return phone;
	}
	public void setPhone(String phone) {
		this.phone = phone;
	}
	public Double getpGrid() {
		return pGrid;
	}
	public void setpGrid(Double pGrid) {
		this.pGrid = pGrid;
	}
	public Double getqGrid() {
		return qGrid;
	}
	public void setqGrid(Double qGrid) {
		this.qGrid = qGrid;
	}
	public Double getpLoad() {
		return pLoad;
	}
	public void setpLoad(Double pLoad) {
		this.pLoad = pLoad;
	}
	public Double getqLoad() {
		return qLoad;
	}
	public void setqLoad(Double qLoad) {
		this.qLoad = qLoad;
	}
	public Double getpBatts() {
		return pBatts;
	}
	public void setpBatts(Double pBatts) {
		this.pBatts = pBatts;
	}
	public Double getqBatts() {
		return qBatts;
	}
	public void setqBatts(Double qBatts) {
		this.qBatts = qBatts;
	}
	public Double getpPV() {
		return pPV;
	}
	public void setpPV(Double pPV) {
		this.pPV = pPV;
	}
	public Double getqPV() {
		return qPV;
	}
	public void setqPV(Double qPV) {
		this.qPV = qPV;
	}
	public Double getpEV() {
		return pEV;
	}
	public void setpEV(Double pEV) {
		this.pEV = pEV;
	}
	public Double getqEV() {
		return qEV;
	}
	public void setqEV(Double qEV) {
		this.qEV = qEV;
	}
	public Double getPower4Grid() {
		return power4Grid;
	}
	public void setPower4Grid(Double power4Grid) {
		this.power4Grid = power4Grid;
	}
	public Double getPower2Grid() {
		return power2Grid;
	}
	public void setPower2Grid(Double power2Grid) {
		this.power2Grid = power2Grid;
	}
	public Double getPowerLoad() {
		return powerLoad;
	}
	public void setPowerLoad(Double powerLoad) {
		this.powerLoad = powerLoad;
	}
	public Double getPowerCharge() {
		return powerCharge;
	}
	public void setPowerCharge(Double powerCharge) {
		this.powerCharge = powerCharge;
	}
	public Double getPowerDisCharge() {
		return powerDisCharge;
	}
	public void setPowerDisCharge(Double powerDisCharge) {
		this.powerDisCharge = powerDisCharge;
	}
	public Double getPowerPV() {
		return powerPV;
	}
	public void setPowerPV(Double powerPV) {
		this.powerPV = powerPV;
	}
	public Double getPowerEV() {
		return powerEV;
	}
	public void setPowerEV(Double powerEV) {
		this.powerEV = powerEV;
	}
	public Integer getHealthStat() {
		return healthStat;
	}
	public void setHealthStat(Integer healthStat) {
		this.healthStat = healthStat;
	}
	public Date getCrtTim() {
		return crtTim;
	}
	public void setCrtTim(Date crtTim) {
		this.crtTim = crtTim;
	}
	
	
	
	
}
