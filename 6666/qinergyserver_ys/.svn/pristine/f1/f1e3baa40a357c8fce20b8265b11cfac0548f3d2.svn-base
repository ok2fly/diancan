.header{
		height : 170px;
		position: relative;
}
@media screen and (min-width:1920px){
	.header{
		height : 170px;
		position: relative;
	}
}
@media screen and (max-width:1920px){
	.header{
		height : 170px;
		position: relative;
	}
}
@media screen and (max-width:1600px){
	.header{
		height : 120px;
		position: relative;
	}
}
@media screen and (max-width:1440px){
	.header{
		height : 110px;
		position: relative;
	}
}
@media screen and (max-width:1360px){
	.header{
		height : 110px;
		position: relative;
	}
}
.logo{
    display: inline-block;
    position: absolute;
    top: 18px;
    left: 10px;
    width: 340px;
}

.system-name{
    color: #fff;
    position: absolute;
    top: 35px;
    left: 360px;
    font-size: 34px;
}

.test{
	display : none;
	position: absolute;
    top: 53px;
    right: 27px;
    width: 0;
    height: 0;
    border-top: 15px solid transparent;
    border-right: 15px solid rgba(255,255,255,0.8);
    border-bottom: 0px solid transparent;
    z-index: 100;
}
.head-menu{
	display : none;
	position: absolute;
    top: 75px;
    right: 25px;
    width: 85px;
    height: 80px;
    z-index: 100;
}
.head-menu li{
	height:35px;
	line-height:35px;
	background : rgba(255,255,255,0.8);
	border-bottom : 1px solid rgba(21,24,39,0.8);
	text-align: center;
	color : #131927;
}
.head-menu li a{
	color : #131927;
}
.headrigh{
	position: absolute;
    top: 30px;
    right: 10px;
    background: url(../images/gr.png) no-repeat;
	background-size: 40px;
	width : 70px;
    height: 40px;
    cursor: pointer;
}
.headrigh-sys{
	position: absolute;
    top: 30px;
    right: 110px;
    height: 40px;
    color: #fff;
    font-size: 18px;
    line-height: 40px;
}
 
.headrigh-sys span a{
	font-size: 18px;
    color: #fff;
}
.headrigh-sys span a:hover{
    color: #32727C;
}
.main{
	min-width:1150px;
	margin:10px auto 0;
}
.mainleft{
	width: 60%;
    float: left;
}


.main-item{
    float: left;
}

.main-item li{
	margin :15px 5px;
	width: 250px;
	height:100px;
	padding :10px;
	position: relative;
}
.main-item li:hover {
	transform: scale(1.05);  
}
.main-item li a{
    font-size: 22px;
    color: #fff;
    line-height: 93px;
    margin-left: 10px;
}

.main-item li.hei a{
    font-size: 22px;
    color: #fff;
    line-height: 443px;
    margin-left: 10px;
}

.main-item li.wid a,.main-item li.wid1 a{
    font-size: 20px;
    color: #fff;
    line-height: 175px;
    margin-left: 0;
}
	
.main-item li i {
    display: inline-block;
    width: 85px;
    height: 85px;
    position: absolute;
    right: 20px;
    top: 23px;
    background-size: 100%;
}

.main-item .bd1 i{
	background: url(../images/xtglicon.png) no-repeat;
	background-size: 85px;
}

.main-item .bd2 i{
	background: url(../images/ywglicon.png) no-repeat;
	background-size : 85px;
}

.main-item .bd3 i{
	background: url(../images/sbglicon.png) no-repeat;
}

.main-item .bd4 i{
	background: url(../images/yyglicon.png) no-repeat;
}

.main-item .bd5 i{
	
	background: url(../images/zhjkicon.png) no-repeat;
	background-size : 85px;
    right: 94px;
    top: 75px;
}

.main-item .bd6 i{
	background: url(../images/ryglicon.png) no-repeat;
}

.main-item .bd7 i{
	background: url(../images/xtgl.png) no-repeat;
}
.main-item .bd8 i{
	background: url(../images/aqgl.png) no-repeat; 
}

.main-item .bd9 i{
	background: url(../images/zsk.png) no-repeat;
    right: 5px;
    top: 15px;
}

.main-item .bd10 i{
	background: url(../images/bg.png) no-repeat;
	right: 20px;
    top: 23px;
}
.main-item .bd11 i{
	background: url(../images/dp.png) no-repeat;
	background-size: 85px;
    top: 0;
    right: 25px;
}
.main-item .bd12 i{
	background: url(../images/dp.png) no-repeat;
	background-size: 85px;
    top: 0;
    right: 25px;
}
.main-item .bd13 i{
	background: url(../images/dp.png) no-repeat;
	background-size: 85px;
    top: 0;
    right: 25px;
}
.main-item .bd14 i{
	background: url(../images/jqqd.png) no-repeat;
	background-size: 85px;
    top: 0;
    right: 25px;
}

.main-item li.hei{
	height:235px;
}

.main-item li.wid{
	width:112px;
	float:left;
	margin-top: 0;
    /* margin-left: 0; */
}

.main-item li.wid1{
	width:112px;
	float:left;
	margin-top: 0;
    margin-left: 0; 
}
.mainright li{
	margin :15px 5px;
	width: 460px;
	height:235px;
	padding :10px;
	position: relative;
}


