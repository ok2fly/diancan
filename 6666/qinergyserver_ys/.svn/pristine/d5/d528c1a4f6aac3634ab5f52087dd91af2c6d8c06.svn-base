.hea-table{
    float: left;
    margin-left: 1%;
    margin-top: 18px;
    width: 95%;
    height: 395px;
	border: solid 1px #CCCCCC;
	background: #fff;
    border-radius: 3px;
}

 
.hea-table th {
	background: #F8F8F8;
	height:30px;
	line-height : 30px;
	border-bottom: solid 1px #CCCCCC;
	position: relative;
	border-right: 1px solid #ccc;
	border-top: 1px solid #ccc;
}
  
.hea-table td {
	padding: 5px 0;
	height: 15px;
	line-height: 10px;
	border-bottom: solid 1px #CCCCCC;
	border-right: 1px solid #ccc;
	text-align: center;
} 
.hea-table th:last-child {
	border-right: none;
} 
.hea-table td:last-child {
	border-right: none;
} 
